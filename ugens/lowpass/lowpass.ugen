# lowpass(sound, cutoff)

lowpass(snd: a, cutoff: ab): a
lowpassb(snd: b, cutoff: b): b

FAUST

declare description "Lowpass Unit Generator for Arco";
declare terminate "snd";

import("filters.lib");

process(snd, cutoff) = snd : lowpass(1, cutoff);
