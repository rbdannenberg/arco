class Delay(Ugen):
    def init(chans, inp, dur, fb, maxdur):
        super.init(create_ugen_id(), "Delay", chans, 'a', "UUUf",
	           'inp', inp, 'dur', dur, 'fb', fb, 'maxdur', maxdur)


def delay(inp, dur, fb, maxdur, optional chans = 1):
    Delay(chans, inp, dur, fb, maxdur)  # delay as a function


