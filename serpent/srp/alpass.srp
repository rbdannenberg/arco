class Alpass (Ugen):
    def init(chans, input, dur, fb, maxdur):
        super.init(create_ugen_id(), "Alpass", chans, 'a', "UUUf",
	           'input', input, 'dur', dur, 'fb', fb, 'maxdur', maxdur)


def alpass(input, dur, fb, maxdur, optional chans = 1):
    Alpass(chans, input, dur, fb, maxdur)  # alpass as a function


